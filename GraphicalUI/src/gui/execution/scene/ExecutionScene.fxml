<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.execution.scene.ExecutionController">
   <bottom>
      <ButtonBar BorderPane.alignment="CENTER">
         <buttons>
            <Button fx:id="clearButton" mnemonicParsing="false" text="Clear" />
            <Button fx:id="startButton" mnemonicParsing="false" text="Start" />
         </buttons>
      </ButtonBar>
   </bottom>
   <center>
       <SplitPane dividerPositions="0.5" prefHeight="200.0" prefWidth="160.0" BorderPane.alignment="CENTER">
           <BorderPane fx:id="leftPane" prefHeight="200.0" prefWidth="200.0"/>
           <BorderPane fx:id="centerPane" prefHeight="200.0" prefWidth="200.0"/>
       </SplitPane>
   </center>
</BorderPane>
