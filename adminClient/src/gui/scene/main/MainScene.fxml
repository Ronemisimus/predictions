<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fx:id="mainRoot" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.scene.main.MainScene">
    <BorderPane fx:id="content" prefHeight="900.0" prefWidth="1600.0">
        <center>
            <BorderPane fx:id="centerStage" BorderPane.alignment="CENTER" />
        </center>
      <top>
          <AnchorPane BorderPane.alignment="CENTER">
              <GridPane alignment="CENTER" layoutY="2.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                  </rowConstraints>
                  <AnchorPane>
                      <GridPane.margin>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                      </GridPane.margin>
                      <Button fx:id="managementButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                              text="Management" textAlignment="CENTER" AnchorPane.bottomAnchor="5.0"
                              AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0"/>
                  </AnchorPane>
                  <AnchorPane GridPane.columnIndex="1">
                      <GridPane.margin>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                      </GridPane.margin>
                      <Button fx:id="allocationsButton" mnemonicParsing="false" text="Allocations"
                              AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                              AnchorPane.topAnchor="5.0" GridPane.columnIndex="1"/>
                  </AnchorPane>
                  <AnchorPane GridPane.columnIndex="2">
                      <GridPane.margin>
                          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
                      </GridPane.margin>
                      <Button fx:id="executionHistoryButton" mnemonicParsing="false" text="Execution History" AnchorPane.bottomAnchor="5.0"
                              AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0"
                              GridPane.columnIndex="2"/>
                  </AnchorPane>
              </GridPane>
          </AnchorPane>
      </top>
    </BorderPane>
</ScrollPane>
