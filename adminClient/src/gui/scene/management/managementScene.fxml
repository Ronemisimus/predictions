<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="193.0" prefWidth="787.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.scene.management.ManagementScene">
   <TitledPane fx:id="titledPane" collapsible="false" contentDisplay="RIGHT" text="Management Panel"
               textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <graphic>
         <AnchorPane fx:id="toolbar" maxWidth="10000.0" prefHeight="24.0" prefWidth="640.0">
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="121.0" minWidth="10.0" prefWidth="86.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0" prefWidth="121.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="10.0" prefWidth="94.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="182.0" minWidth="10.0" prefWidth="110.0"/>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="186.0" minWidth="10.0" prefWidth="179.0"/>
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
               </rowConstraints>
               <AnchorPane GridPane.columnSpan="5">
                  <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                     </rowConstraints>
                     <HBox alignment="CENTER" GridPane.columnIndex="4">
                        <Button fx:id="loadFileButton" contentDisplay="CENTER" mnemonicParsing="false"
                                text="Load File" textAlignment="CENTER"/>
                     </HBox>
                     <Hyperlink fx:id="filePathHolder" prefHeight="22.0" prefWidth="404.0"
                                text="Loaded file path will appear here" GridPane.columnSpan="4"/>
                  </GridPane>
               </AnchorPane>
               <HBox alignment="CENTER_RIGHT" GridPane.columnIndex="5">
                  <Button fx:id="setThreadCountButton" mnemonicParsing="false" text="Set Thread Count"/>
               </HBox>
            </GridPane>
         </AnchorPane>
      </graphic>
      <SplitPane dividerPositions="0.333, 0.666" prefHeight="160.0" prefWidth="200.0">
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0"/>
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="232.0"/>
         <AnchorPane layoutX="10.0" layoutY="10.0" minHeight="0.0" minWidth="0.0" prefHeight="160.0"
                     prefWidth="161.0"/>
      </SplitPane>
   </TitledPane>
</AnchorPane>
