<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>main.login.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientLogin</servlet-name>
        <servlet-class>main.login.ClientLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>readFile</servlet-name>
        <servlet-class>main.ReadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminSetThread</servlet-name>
        <servlet-class>main.thread.adminSet.AdminSetThread</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminRequests</servlet-name>
        <servlet-class>main.requests.servlets.adminRequests</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getLoadedWorlds</servlet-name>
        <servlet-class>main.getLoadedWorlds.getLoadedWorlds</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getRunStates</servlet-name>
        <servlet-class>main.getRunStates</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userRequests</servlet-name>
        <servlet-class>main.requests.servlets.userRequests</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userRequests</servlet-name>
        <url-pattern>/requests</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getRunStates</servlet-name>
        <url-pattern>/getRunStates</url-pattern>
        <url-pattern>/admin/getRunStates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getLoadedWorlds</servlet-name>
        <url-pattern>/getLoadedWorlds</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminRequests</servlet-name>
        <url-pattern>/admin/requests</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/admin/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>readFile</servlet-name>
        <url-pattern>/admin/readFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminSetThread</servlet-name>
        <url-pattern>/admin/setThreadCount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientLogin</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>